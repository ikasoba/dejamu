{
  "lock": false,
  "tasks": {
    "dejamu": "echo \"import 'dejamu/cli/cli.ts';\" | deno run -A -",
    "build": "deno task dejamu build",
    "serve": "deno task dejamu serve"
  },
  "imports": {
    "dejamu/": "https://raw.githubusercontent.com/ikasoba/dejamu/pre-0.2.0+0002/",
    "path/": "https://deno.land/std@0.203.0/path/",
    "@twind/": "npm:/@twind/",
    "react": "npm:/preact/compat"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact"
  }
}
